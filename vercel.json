{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "crons": [
    {
      "path": "/api/datasets/cleanup",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/keep-alive",
      "schedule": "0 3 * * *"
    }
  ],
  "functions": {
    "app/api/chat/[datasetId]/route.ts": {
      "maxDuration": 600,
      "memory": 1024
    },
    "app/api/ingest/route.ts": {
      "maxDuration": 180,
      "memory": 1024
    },
    "app/api/report/generate/route.ts": {
      "maxDuration": 400,
      "memory": 1024
    },
    "app/api/datasets/cleanup/route.ts": {
      "maxDuration": 60,
      "memory": 1024
    },
    "app/api/keep-alive/route.ts": {
      "maxDuration": 10,
      "memory": 128
    }
  }
}
